generator client {
  provider = "prisma-client-js"
  output   = "../src/app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Topper {
  id         Int     @id @default(autoincrement())
  name       String
  rank       String?
  percentage String?
  avatar     String
  batch      Int
  exam       String
}
